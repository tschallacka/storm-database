{
    "name": "tschallacka/storm-database",
    "description": "The WinterCMS Storm library but only the database features",
    "version": "0.1",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "meyfa/phpunit-assert-gd": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Winter\\Storm\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/DbTestCase.php"
        ]
    },
    "scripts": {
        "test": [
            "phpunit -c tests/phpunit.xml --stop-on-failure"
        ],
        "update-from-source": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || bash bin/sync.sh"
        ]
    },
    "license": "MIT",
    "require": {
        "illuminate/database": "~6.0",
        "symfony/mime": "~4.4.0",
        "symfony/http-foundation": "~4.4.0",
        "illuminate/events": "~6.0",
        "illuminate/encryption": "~6.0",
        "illuminate/translation": "~6.0",
        "illuminate/validation": "~6.0"
    }
}
